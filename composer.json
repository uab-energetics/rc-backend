{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-cors": "^0.10.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "enqueue/amqp-lib": "^0.8.23",
        "enqueue/laravel-queue": "^0.8.4",
        "fideloper/proxy": "~3.3",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "lcobucci/jwt": "^3.2",
        "php-amqplib/php-amqplib": "^2.7",
        "phpseclib/phpseclib": "^2.0",
        "pusher/pusher-php-server": "^3.0",
        "sentry/sentry": "^1.8",
        "sentry/sentry-laravel": "^0.8.0",
        "tymon/jwt-auth": "^0.5.12"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php",
            "app/definitions.php",
            "app/routeFactory.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "deploy-prod": "ssh researchcoder.com 'cd /var/www/v3/api.researchcoder.com; git checkout -- .; composer build'",
        "deploy-dev": "ssh researchcoder.com 'cd /var/www/dev/api.dev.researchcoder.com; git checkout -- .; composer build'",
        "build" : "git pull; composer install -o; php artisan migrate --no-interaction"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
